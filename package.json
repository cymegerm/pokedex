{
  "name": "pokedex",
  "version": "0.0.0",
  "repository": "git@github.perso:cymegerm/pokedex.git",
  "author": "Simon St-Germain <cymegerm@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=development --proxy-config ./proxy.conf.json",
    "server": "ts-node -P ./server/server.tsconfig.json ./server/server.ts",
    "build": "ng build",
    "test": "tsc -p src/tsconfig.spec.json && jest --coverage",
    "test:watch": "tsc -p src/tsconfig.spec.json && jest --clearCache && jest --watch",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "lint-ts": "ng lint",
    "lint-scss": "stylelint 'src/**/*.scss' --cache --cache-location './.stylelintcache/'",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "prettier-write": "prettier --write '**/*.{ts,js,css,html}'",
    "prettier-check": "prettier --check '**/*.{ts,js,css,html}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn lint-ts && yarn lint-scss && yarn test",
      "pre-push": "ng build --aot true"
    }
  },
  "dependencies": {
    "@angular/common": "^11.0.5",
    "@angular/compiler": "^11.0.5",
    "@angular/core": "^11.0.5",
    "@angular/platform-browser": "^11.0.5",
    "@angular/platform-browser-dynamic": "^11.0.5",
    "@angular/router": "^11.0.5",
    "@ngrx/effects": "^10.0.1",
    "@ngrx/router-store": "^10.0.1",
    "@ngrx/store": "^10.0.1",
    "@ngrx/store-devtools": "^10.0.1",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^10.0.1",
    "@angular-devkit/build-angular": "^0.1100.5",
    "@angular/cli": "^11.0.5",
    "@angular/compiler-cli": "^11.0.5",
    "@angular/language-service": "^11.0.5",
    "@briebug/cypress-schematic": "^4.3.0",
    "@cypress/webpack-preprocessor": "^5.4.10",
    "@ngrx/schematics": "^10.0.1",
    "@types/express": "^4.17.9",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^26.0.19",
    "@types/node": "12.12.58",
    "axios": "^0.20.0",
    "codelyzer": "^6.0.0",
    "cypress": "^5.5.0",
    "express": "^4.17.1",
    "husky": "^4.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.3.2",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-prettier": "^1.1.2",
    "ts-loader": "^8.0.12",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "4.0.5",
    "wallaby-webpack": "^3.9.16"
  }
}
